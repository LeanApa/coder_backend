

title, description, code, price, status, stock = true, category,thumbnails

{{#each products}}
<div class="card" style="width: 18rem;">
  <img src="..." class="card-img-top" alt={{this.thumbnails}}>
  <div class="card-body">
    <h5 class="card-title">{{this.title}}</h5>
    <p class="card-text">{{this.category}}</p>
    <p class="card-text">{{this.description}}</p>
    <p class="card-text">{{this.code}}</p>
    <p class="card-text">{{this.price}}</p>
    <p class="card-text">{{this.status}}</p>
    <p class="card-text">{{this.stock}}</p>
    
  </div>
</div>
{{/each}}